<COMPONENT>
	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20050320A"
		STARTABLE="true"
		SCHEMA_AUTO_PROPAGATE="false"
		PARTITIONING="NONE"
	>
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/PostgreSQL</FAMILY>
		<FAMILY>Business_Intelligence/DB_SCD</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER
			NAME="PROPERTY"
			FIELD="PROPERTY_TYPE"
			SHOW="true"
			NUM_ROW="10"
			REPOSITORY_VALUE="DATABASE:POSTGRESQL"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		/>

		<PARAMETER
			NAME="USE_EXISTING_CONNECTION"
			FIELD="CHECK"
		NUM_ROW="11">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="CONNECTION"
			FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tPostgresqlConnection"
			NUM_ROW="11"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			DYNAMIC_SETTINGS="true"
		/>

		<PARAMETER
			NAME="DB_VERSION"
			FIELD="CLOSED_LIST"
			NUM_ROW="15"
			REPOSITORY_VALUE="DB_VERSION"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="PRIOR_TO_V9">
				<ITEM NAME="PRIOR_TO_V9" VALUE="PRIOR_TO_V9" />
				<ITEM NAME="V9_X" VALUE="V9_X" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="HOST"
			FIELD="TEXT"
			NUM_ROW="30"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SERVER_NAME"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="PORT"
			FIELD="TEXT"
			NUM_ROW="30"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PORT"
		>
			<DEFAULT>"5432"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="DBNAME"
			FIELD="TEXT"
			NUM_ROW="31"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="SID"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SCHEMA_DB"
			FIELD="TEXT"
			NUM_ROW="31"
			REPOSITORY_VALUE="SCHEMA"
			REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
 
		<PARAMETER
			NAME="TYPE"
			FIELD="TEXT"
			NUM_ROW="31"
			SHOW="false"
			REPOSITORY_VALUE="TYPE"
		>
			<DEFAULT>Postgresql</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USER"
			FIELD="TEXT"
			NUM_ROW="40"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="USERNAME"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="PASS"
			FIELD="PASSWORD"
			NUM_ROW="40"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REPOSITORY_VALUE="PASSWORD"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TABLE_SRC"
			FIELD="TEXT"
			NUM_ROW="50"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TABLE"
			FIELD="DBTABLE"
			NUM_ROW="51"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TABLE_ACTION"
			FIELD="CLOSED_LIST"
			NUM_ROW="52"
			NOT_SHOW_IF="PARALLELIZE == 'true'"
		>
			<ITEMS DEFAULT="NONE">
				<ITEM NAME="NONE" VALUE="NONE"/>
				<ITEM NAME="CREATE" VALUE="CREATE"/>
				<ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
				<ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
				<ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
				<ITEM NAME="CLEAR" VALUE="CLEAR"/>
				<ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="60"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SURROGATE_KEY"
			FIELD="COLUMN_LIST"
			NUM_ROW="61"
			SHOW="true"
		>
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SK_CREATION"
			FIELD="CLOSED_LIST"
			NUM_ROW="61"
			SHOW="true"
		>
			<ITEMS DEFAULT="AUTO_INCREMENT">
				<ITEM NAME="AUTO_INCREMENT"	VALUE="AUTO_INCREMENT" />
				<ITEM NAME="DB_SEQUENCE"	VALUE="DB_SEQUENCE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="SK_DB_SEQUENCE"
			FIELD="TEXT"
			NUM_ROW="61"
			SHOW_IF="SK_CREATION == 'DB_SEQUENCE'"
			SHOW="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SOURCE_KEYS"
			FIELD="TABLE"
			NUM_ROW="62"
			NB_LINES="2"
		>
			<ITEMS>
				<ITEM NAME="NAME" FIELD="COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="DROP_TMP"
			FIELD="CHECK"
			NUM_ROW="69"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L1"
			FIELD="CHECK"
			NUM_ROW="70"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L1_FIELDS"
			FIELD="TABLE"
			NUM_ROW="71"
			NB_LINES="5"
			SHOW_IF="USE_L1 == 'true'"
		>
			<ITEMS>
				<ITEM NAME="NAME" FIELD="COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L2"
			FIELD="CHECK"
			NUM_ROW="80"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L2_FIELDS"
			FIELD="TABLE"
			NUM_ROW="81"
			NB_LINES="5"
			SHOW_IF="USE_L2 == 'true'"
		>
			<ITEMS>
				<ITEM NAME="NAME" FIELD="COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="L2_STARTDATE_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="82"
			SHOW_IF="USE_L2 == 'true'"
		>
		</PARAMETER>

		<PARAMETER
			NAME="L2_ENDDATE_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="82"
			SHOW_IF="USE_L2 == 'true'"
		>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L2_ACTIVE"
			FIELD="CHECK"
			NUM_ROW="83"
			SHOW_IF="USE_L2 == 'true'"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L2_ACTIVE_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="83"
			SHOW_IF="(USE_L2_ACTIVE == 'true') and (USE_L2 == 'true')"
		>
		</PARAMETER>

		<PARAMETER
			NAME="USE_L2_VERSION"
			FIELD="CHECK"
			NUM_ROW="84"
			SHOW_IF="USE_L2 == 'true'"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="L2_VERSION_FIELD"
			FIELD="COLUMN_LIST"
			NUM_ROW="84"
			SHOW_IF="(USE_L2_VERSION == 'true') and (USE_L2 == 'true')"
		>
		</PARAMETER>

		<PARAMETER
			NAME="MAPPING"
			FIELD="MAPPING_TYPE"
			NUM_ROW="90"
			SHOW="false"
		>
			<DEFAULT>postgres_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER
			NAME="ENCODING"
			FIELD="ENCODING_TYPE"
			NUM_ROW="10"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			REQUIRED="true"
			SHOW="false"
		>
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="DEBUG"
			FIELD="CHECK"
			NUM_ROW="20"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Driver-Postgres9" MODULE="postgresql-9.4-1201.jdbc41.jar" MVN="mvn:org.talend.libraries/postgresql-9.4-1201.jdbc41/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.jdbc.postgresql/lib/postgresql-9.4-1201.jdbc41.jar" REQUIRED_IF="(DB_VERSION =='V9_X') AND (USE_EXISTING_CONNECTION == 'false')" />
			<IMPORT NAME="Driver-Postgres" MODULE="postgresql-8.3-603.jdbc3.jar" MVN="mvn:org.talend.libraries/postgresql-8.3-603.jdbc3/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.jdbc.postgresql/lib/postgresql-8.3-603.jdbc3.jar" REQUIRED_IF="(DB_VERSION =='PRIOR_TO_V9') AND (USE_EXISTING_CONNECTION == 'false')" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
