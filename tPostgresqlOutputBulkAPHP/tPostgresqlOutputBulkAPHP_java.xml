<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/PostgreSQL</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="1" REPOSITORY_VALUE="DELIMITED" />

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="2" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="3" REPOSITORY_VALUE="APPEND">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="5">
      <DEFAULT/> 
    </PARAMETER>
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   	<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="ROW_SEPARATOR">
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

		<PARAMETER NAME="SKIP_SERIAL" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="6">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ONLY_KEY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="6">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DATE_FORMAT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="6">
			<DEFAULT>"yyyy-MM-dd HH:mm:ss"</DEFAULT>
		</PARAMETER>
	<PARAMETER NAME="INCLUDEHEADER" FIELD="CHECK" REQUIRED="true"
		NUM_ROW="7" SHOW_IF="(FILETYPE=='CSVFILE')" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="FILETYPE" FIELD="CLOSED_LIST" NUM_ROW="8">
		<ITEMS DEFAULT="CSVFILE">
            <ITEM NAME="BINARYFILE" VALUE="BINARYFILE" />
			<ITEM NAME="TXTFILE" VALUE="TXTFILE" />
			 <ITEM NAME="CSVFILE" VALUE="CSVFILE" /> 
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="NULLSTRING" FIELD="TEXT" NUM_ROW="9"
		REQUIRED="true" SHOW_IF="(FILETYPE=='CSVFILE' or FILETYPE=='TXTFILE')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="9" MAX_LENGTH="1"
		REQUIRED="true" SHOW_IF="(FILETYPE=='CSVFILE' or FILETYPE=='TXTFILE')">
		<DEFAULT>";"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="ESCAPE_CHAR" FIELD="TEXT" NUM_ROW="9"
		SHOW_IF="FILETYPE=='CSVFILE'">
		<DEFAULT>"\\"</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" NUM_ROW="9" MAX_LENGTH="1"
		SHOW_IF="FILETYPE=='CSVFILE'">
		<DEFAULT>"\""</DEFAULT>
	</PARAMETER>

    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="12" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"UTF-8"</DEFAULT>
    </PARAMETER>
   </ADVANCED_PARAMETERS>

	 <CODEGENERATION>
	<IMPORTS>
		<IMPORT  NAME="JODATIME" MODULE="joda-time-2.9.3.jar" REQUIRED="true"/>
	</IMPORTS>
  </CODEGENERATION>

   <RETURNS>
     <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
