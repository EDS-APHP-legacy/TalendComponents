<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070320A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/PostgreSQL</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="DATABASE:POSTGRESQL"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tPostgresqlConnection"
			NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true"/>

		<PARAMETER
			NAME="DB_VERSION"
			FIELD="CLOSED_LIST"
			NUM_ROW="5"
			REPOSITORY_VALUE="DB_VERSION"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="V9_X">
				<ITEM NAME="V9_X" VALUE="V9_X" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="10"
			REPOSITORY_VALUE="SERVER_NAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TYPE"
			FIELD="TEXT"
			NUM_ROW="10"
			SHOW="false"
			REPOSITORY_VALUE="TYPE"
		>
			<DEFAULT>PostgreSQL</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="10"
			REPOSITORY_VALUE="PORT"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"5432"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="15"
			REPOSITORY_VALUE="SID"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_DB" FIELD="TEXT" NUM_ROW="15"
			REPOSITORY_VALUE="SCHEMA"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="20"
			REPOSITORY_VALUE="USERNAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="PASSWORD" NUM_ROW="20"
			REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="25">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE_ACTION" FIELD="CLOSED_LIST"
			NUM_ROW="35" NOT_SHOW_IF="PARALLELIZE == 'true'">
			<ITEMS DEFAULT="NONE">
				<ITEM NAME="NONE" VALUE="NONE" />
				<ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE" />
				<ITEM NAME="CREATE" VALUE="CREATE" />
				<ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS" />
				<ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
				<ITEM NAME="CLEAR" VALUE="CLEAR" />
				<ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="35"
				NOT_SHOW_IF="(TABLE_ACTION == 'NONE') OR (USE_EXISTING_CONNECTION == 'false')"
			>
			<DEFAULT>Warning : this component configuration will automatically generate a commit before bulk insert</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SELECT_JOIN_FIELD"
			FIELD="TEXT"
			NUM_ROW="36"
			REQUIRED="false"
			SHOW_IF="DATA_ACTION=='SELECT_JOIN'">
			<DEFAULT>"\"id\", \"d8\", \"val\""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="40"
			REQUIRED="true">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FILENAME_OUT" FIELD="FILE" NUM_ROW="40"
			REQUIRED="true">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="45">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="50"
			SHOW="false">
			<DEFAULT>postgres_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER
			NAME="DATA_ACTION"
			FIELD="CLOSED_LIST"
			NUM_ROW="4"
		>
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT"/>
				<ITEM NAME="UPDATE" VALUE="UPDATE"/>
				<ITEM NAME="DELSERT" VALUE="DELSERT"/>
				<ITEM NAME="DELETE" VALUE="DELETE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="OIDS" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SKIP_SERIAL" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="6" SHOW_IF="DATA_ACTION=='INSERT'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ONLY_KEY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="6" SHOW_IF="DATA_ACTION=='DELETE'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="HEADER" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="7" SHOW_IF="(FILETYPE=='CSVFILE')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FILETYPE" FIELD="CLOSED_LIST" NUM_ROW="8">
			<ITEMS DEFAULT="CSVFILE">
				<ITEM NAME="BINARYFILE" VALUE="BINARYFILE" />
				<ITEM NAME="TXTFILE" VALUE="TXTFILE" />
				<ITEM NAME="CSVFILE" VALUE="CSVFILE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="NULLSTRING" FIELD="TEXT" NUM_ROW="9"
			REQUIRED="true"
			SHOW_IF="(FILETYPE=='CSVFILE' or FILETYPE=='TXTFILE')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FIELDS_TERMINATED_BY" FIELD="TEXT" NUM_ROW="9"
			REQUIRED="true" MAX_LENGTH="1"
			SHOW_IF="(FILETYPE=='CSVFILE' or FILETYPE=='TXTFILE')">
			<DEFAULT>";"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ESCAPE_CHAR" FIELD="TEXT" NUM_ROW="9"
			SHOW_IF="FILETYPE=='CSVFILE'">
			<DEFAULT>"\\"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" NUM_ROW="9" MAX_LENGTH="1"
			SHOW_IF="FILETYPE=='CSVFILE'">
			<DEFAULT>"\""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STANDARD_CONFORMING_STRING" FIELD="CHECK" NUM_ROW="10" REQUIRED="true"
			SHOW_IF="FILETYPE=='CSVFILE'">
			<DEFAULT IF="(DB_VERSION=='V9_X') AND (FILETYPE=='CSVFILE')">true</DEFAULT>
		</PARAMETER>


		<PARAMETER NAME="FORCENOTNULL" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="11" NB_LINES="5" SHOW_IF="(FILETYPE=='CSVFILE')">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="FORCENOTNULL" FIELD="CHECK" VALUE="false" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Driver-Postgres9" MODULE="postgresql-9.2-1003.jdbc3.jar" UrlPath="platform:/plugin/org.talend.libraries.jdbc.postgresql/lib/postgresql-9.2-1003.jdbc3.jar" REQUIRED_IF="(DB_VERSION =='V9_X') AND (USE_EXISTING_CONNECTION == 'false')" />
		</IMPORTS>
	</CODEGENERATION>
  <RETURNS>
      <RETURN NAME="NB_LINE_MODIFIED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_UPDATED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_DELETED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="AFTER"/>
   </RETURNS>

</COMPONENT>
