<COMPONENT>

	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20070320A"
		STARTABLE="false"
		PARTITIONING="NONE"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/PostgreSQL</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="DATABASE:POSTGRESQL"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tPostgresqlConnection"
			NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true"/>

		<PARAMETER
			NAME="DB_VERSION"
			FIELD="CLOSED_LIST"
			NUM_ROW="5"
			REPOSITORY_VALUE="DB_VERSION"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="V9_X">
				<ITEM NAME="V9_X" VALUE="V9_X" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="10"
			REPOSITORY_VALUE="SERVER_NAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TYPE"
			FIELD="TEXT"
			NUM_ROW="10"
			SHOW="false"
			REPOSITORY_VALUE="TYPE"
		>
			<DEFAULT>PostgreSQL</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="10"
			REPOSITORY_VALUE="PORT"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"5432"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="15"
			REPOSITORY_VALUE="SID"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_DB" FIELD="TEXT" NUM_ROW="15"
			REPOSITORY_VALUE="SCHEMA"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="20"
			REPOSITORY_VALUE="USERNAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="PASSWORD" NUM_ROW="20"
			REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="25">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="DATA_ACTION"
			FIELD="CLOSED_LIST"
			NUM_ROW="26"
		>
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT"/>
				<ITEM NAME="UPDATE" VALUE="UPDATE"/>
				<ITEM NAME="DELSERT" VALUE="DELSERT"/>
				<ITEM NAME="DELETE" VALUE="DELETE"/>
				<ITEM NAME="SELECT_JOIN" VALUE="SELECT_JOIN"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SKIP_SERIAL" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="27"
			SHOW_IF="DATA_ACTION=='INSERT'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATE_FORMAT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="27">
			<DEFAULT>"yyyy-MM-dd HH:mm:ss"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ONLY_KEY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="27"
			SHOW_IF="(DATA_ACTION=='DELETE') OR (DATA_ACTION=='SELECT_JOIN')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TABLE_ACTION" FIELD="CLOSED_LIST"
			NUM_ROW="35" NOT_SHOW_IF="PARALLELIZE == 'true'">
			<ITEMS DEFAULT="NONE">
				<ITEM NAME="NONE" VALUE="NONE" />
				<ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE" />
				<ITEM NAME="CREATE" VALUE="CREATE" />
				<ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS" />
				<ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
				<ITEM NAME="CLEAR" VALUE="CLEAR" />
				<ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="35"
				NOT_SHOW_IF="(TABLE_ACTION == 'NONE') OR (USE_EXISTING_CONNECTION == 'false')"
			>
			<DEFAULT>Warning : this component configuration will automatically generate a commit before bulk insert</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SELECT_JOIN_FIELD"
			FIELD="TEXT"
			NUM_ROW="36"
			REQUIRED="false"
			SHOW_IF="DATA_ACTION=='SELECT_JOIN'">
			<DEFAULT>"\"id\", \"d8\", \"val\""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="FILENAME"
			FIELD="FILE"
			NUM_ROW="35"
			REQUIRED="true"
		>
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/temp.csv"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="FILENAME_OUT"
			FIELD="FILE"
			NUM_ROW="36"
			REQUIRED="true"
			SHOW_IF="DATA_ACTION=='SELECT_JOIN'">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
		</PARAMETER>
	<!-- following property should not be displayed -->

		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="40"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="MAPPING"
			FIELD="MAPPING_TYPE"
			NUM_ROW="45"
			SHOW="false"
		>
			<DEFAULT>postgres_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER
			NAME="OIDS"
			FIELD="CHECK"
			REQUIRED="true"
			NUM_ROW="6"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="HEADER"
			FIELD="CHECK"
			REQUIRED="true"
			NUM_ROW="7"
			SHOW_IF="(FILETYPE=='CSVFILE')"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="8" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"UTF-8"</DEFAULT>
		</PARAMETER>

				<PARAMETER
			NAME="FILETYPE"
			FIELD="CLOSED_LIST"
			NUM_ROW="14"
		>
			<ITEMS DEFAULT="CSVFILE">
				<ITEM NAME="BINARYFILE" VALUE="BINARYFILE" />
				<ITEM NAME="TXTFILE" VALUE="TXTFILE" />
				<ITEM NAME="CSVFILE" VALUE="CSVFILE"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="NULLSTRING"
			FIELD="TEXT"
			NUM_ROW="15"
			REQUIRED="true"
			SHOW_IF="(FILETYPE=='CSVFILE' or FILETYPE=='TXTFILE')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="15" REPOSITORY_VALUE="ROW_SEPARATOR">
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="FIELDS_TERMINATED_BY"
			FIELD="TEXT"
			NUM_ROW="15"
			REQUIRED="true"
			MAX_LENGTH="1"
			SHOW_IF="(FILETYPE=='CSVFILE' or FILETYPE=='TXTFILE')"
		>
			<DEFAULT>";"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="ESCAPE_CHAR"
			FIELD="TEXT"
			NUM_ROW="15"
			SHOW_IF="FILETYPE=='CSVFILE'"
		>
			<DEFAULT>"\\"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="TEXT_ENCLOSURE"
			FIELD="TEXT"
			NUM_ROW="15"
			MAX_LENGTH="1"
			SHOW_IF="FILETYPE=='CSVFILE'"
		>
			<DEFAULT>"\""</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="STANDARD_CONFORMING_STRING" FIELD="CHECK" NUM_ROW="16" REQUIRED="true"
			SHOW_IF="FILETYPE=='CSVFILE'">
			<DEFAULT IF="(DB_VERSION=='V9_X') AND (FILETYPE=='CSVFILE')">true</DEFAULT>
		</PARAMETER>	

 		<PARAMETER
			NAME="FORCENOTNULL"
			FIELD="TABLE"
			REQUIRED="true"
			NUM_ROW="17"
			NB_LINES="5"
			SHOW_IF="(FILETYPE=='CSVFILE')"
		>
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="FORCENOTNULL" FIELD="CHECK" VALUE="false"/>
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<TEMPLATES INPUT="tPOB" OUTPUT="tPBE">

			<TEMPLATE NAME="tPOB" COMPONENT="tPostgresqlOutputBulk">
				<LINK_TO NAME="tPBE" CTYPE="ROWS_END"/>
			</TEMPLATE>

			<TEMPLATE NAME="tPBE" COMPONENT="tPostgresqlBulkExec"/>

			<TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="tPBE.DB_VERSION" />
			<TEMPLATE_PARAM SOURCE="self.USE_EXISTING_CONNECTION" TARGET="tPBE.USE_EXISTING_CONNECTION" />
			<TEMPLATE_PARAM SOURCE="self.CONNECTION" TARGET="tPBE.CONNECTION" />
			<TEMPLATE_PARAM SOURCE="self.HOST" TARGET="tPBE.HOST" />
			<TEMPLATE_PARAM SOURCE="self.PORT" TARGET="tPBE.PORT"/>
			<TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tPBE.DBNAME" />
			<TEMPLATE_PARAM SOURCE="self.SCHEMA_DB"	TARGET="tPBE.SCHEMA_DB" />
			<TEMPLATE_PARAM SOURCE="self.USER" TARGET="tPBE.USER" />
			<TEMPLATE_PARAM SOURCE="self.PASS" TARGET="tPBE.PASS" />
			<TEMPLATE_PARAM SOURCE="self.TABLE"	TARGET="tPBE.TABLE" />
			<TEMPLATE_PARAM SOURCE="self.TABLE_ACTION" TARGET="tPBE.TABLE_ACTION" />
			<TEMPLATE_PARAM SOURCE="self.DATA_ACTION" TARGET="tPBE.DATA_ACTION" />
			<TEMPLATE_PARAM SOURCE="self.OIDS" TARGET="tPBE.OIDS" />
			<TEMPLATE_PARAM SOURCE="self.SKIP_SERIAL" TARGET="tPBE.SKIP_SERIAL" />
			<TEMPLATE_PARAM SOURCE="self.SKIP_SERIAL" TARGET="tPOB.SKIP_SERIAL" />
			<TEMPLATE_PARAM SOURCE="self.DATE_FORMAT" TARGET="tPOB.DATE_FORMAT" />
			<TEMPLATE_PARAM SOURCE="self.ONLY_KEY" TARGET="tPBE.ONLY_KEY" />
			<TEMPLATE_PARAM SOURCE="self.ONLY_KEY" TARGET="tPOB.ONLY_KEY" />
			<TEMPLATE_PARAM SOURCE="self.HEADER" TARGET="tPBE.HEADER" />
			<TEMPLATE_PARAM SOURCE="self.HEADER" TARGET="tPOB.INCLUDEHEADER" />
			<TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPOB.FILENAME" />
			<TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPBE.FILENAME" />
			<TEMPLATE_PARAM SOURCE="self.FILENAME_OUT" TARGET="tPBE.FILENAME_OUT" />
			<TEMPLATE_PARAM SOURCE="self.SELECT_JOIN_FIELD" TARGET="tPBE.SELECT_JOIN_FIELD" />
			<TEMPLATE_PARAM SOURCE="self.FILETYPE" TARGET="tPBE.FILETYPE" />
			<TEMPLATE_PARAM SOURCE="self.FILETYPE" TARGET="tPOB.FILETYPE" />
			<TEMPLATE_PARAM SOURCE="self.NULLSTRING" TARGET="tPBE.NULLSTRING" />
			<TEMPLATE_PARAM SOURCE="self.NULLSTRING" TARGET="tPOB.NULLSTRING" />
			<TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tPOB.ROWSEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY" TARGET="tPOB.FIELDSEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY" TARGET="tPBE.FIELDS_TERMINATED_BY" />
			<TEMPLATE_PARAM SOURCE="self.ESCAPE_CHAR" TARGET="tPBE.ESCAPE_CHAR" />
			<TEMPLATE_PARAM SOURCE="self.ESCAPE_CHAR" TARGET="tPOB.ESCAPE_CHAR" />
			<TEMPLATE_PARAM SOURCE="self.TEXT_ENCLOSURE" TARGET="tPBE.TEXT_ENCLOSURE" />
			<TEMPLATE_PARAM SOURCE="self.TEXT_ENCLOSURE" TARGET="tPOB.TEXT_ENCLOSURE" />
			<TEMPLATE_PARAM SOURCE="self.FORCENOTNULL" TARGET="tPBE.FORCENOTNULL" />
			<TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tPBE.SCHEMA" />
			<TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tPOB.SCHEMA" />
			<TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="tPBE.MAPPING" />
			<TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tPOB.ENCODING" />
			<TEMPLATE_PARAM SOURCE="self.STANDARD_CONFORMING_STRING" TARGET="tPBE.STANDARD_CONFORMING_STRING" />
			<TEMPLATE_PARAM SOURCE="self.TYPE" TARGET="tPBE.TYPE" />

		</TEMPLATES>
	</CODEGENERATION>
  <RETURNS>
      <RETURN NAME="tPBE_NB_LINE_MODIFIED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="tPBE_NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="tPBE_NB_LINE_UPDATED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="tPBE_NB_LINE_DELETED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="tPBE_QUERY" TYPE="id_String" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
