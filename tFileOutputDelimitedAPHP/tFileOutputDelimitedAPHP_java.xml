<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070111A"
		STARTABLE="false" PARTITIONING="NONE">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>File/Output</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="10" REPOSITORY_VALUE="DELIMITED" />
		<PARAMETER NAME="USESTREAM" FIELD="CHECK" NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STREAMNAME" FIELD="TEXT" NUM_ROW="20" SHOW_IF="USESTREAM=='true'">
			<DEFAULT>outputStream</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20"
			REQUIRED="true" REPOSITORY_VALUE="FILE_PATH" NOT_SHOW_IF="USESTREAM=='true'">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="30"
			REPOSITORY_VALUE="ROW_SEPARATOR" SHOW_IF="CSV_OPTION=='false'">
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="OS_LINE_SEPARATOR_AS_ROW_SEPARATOR" FIELD="CHECK" REQUIRED="true"
			 NUM_ROW="29" SHOW_IF="CSV_OPTION=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CSVROWSEPARATOR" FIELD="OPENED_LIST" NUM_ROW="30" REPOSITORY_VALUE="ROW_SEPARATOR" MAX_LENGTH="2" SHOW_IF="CSV_OPTION=='true'">
			<ITEMS DEFAULT="LF">
				<ITEM NAME="LF" VALUE="&quot;\n&quot;"></ITEM>
				<ITEM NAME="CR" VALUE="&quot;\r&quot;"></ITEM>
				<ITEM NAME="CRLF" VALUE="&quot;\r\n&quot;"></ITEM>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="30"
			REPOSITORY_VALUE="FIELD_SEPARATOR">
			<DEFAULT>";"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="40" REPOSITORY_VALUE="APPEND" NOT_SHOW_IF="USESTREAM=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="INCLUDEHEADER" FIELD="CHECK" NUM_ROW="40">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	    <PARAMETER NAME="COMPRESS" FIELD="CHECK" NUM_ROW="40" SHOW_IF="(APPEND=='false') and (SPLIT=='false')">
	    	<DEFAULT>false</DEFAULT>
	    </PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="60">
			<DEFAULT />
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="31" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="31"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="31"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CSV_OPTION" FIELD="CHECK" REQUIRED="true"
			REPOSITORY_VALUE="CSV_OPTION" NUM_ROW="35">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ESCAPE_CHAR" FIELD="TEXT" NUM_ROW="35"
			REPOSITORY_VALUE="ESCAPE_CHAR" SHOW_IF="CSV_OPTION == 'true'">
			<DEFAULT>"""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" NUM_ROW="35"
			REPOSITORY_VALUE="TEXT_ENCLOSURE" SHOW_IF="CSV_OPTION == 'true'">
			<DEFAULT>"""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="40" NOT_SHOW_IF="USESTREAM=='true'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SPLIT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50" NOT_SHOW_IF="USESTREAM=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SPLIT_EVERY" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="50" SHOW_IF="(SPLIT == 'true') AND (USESTREAM=='false')">
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FLUSHONROW" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="55">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FLUSHONROW_NUM" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="55" SHOW_IF="FLUSHONROW == 'true'">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ROW_MODE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="70"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"UTF-8"</DEFAULT>
		</PARAMETER>

	  	<PARAMETER NAME="DELETE_EMPTYFILE" FIELD="CHECK" REQUIRED="true" NUM_ROW="80" SHOW_IF="USESTREAM=='false'">
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>
	   <PARAMETER NAME="SCHEMA_OPT_NUM" FIELD="TEXT" NUM_ROW="100" REQUIRED="true" SHOW="false">
            <DEFAULT>90</DEFAULT>
       </PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar"
				REQUIRED="true"/>
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="FILE_NAME" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>
